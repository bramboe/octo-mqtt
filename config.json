{
  "name": "Octo MQTT",
  "version": "1.2.8",
  "slug": "octo-mqtt",
  "description": "A Home Assistant add-on to enable controlling Octo actuators star version 2.",
  "url": "https://github.com/bramboe/octo-mqtt.git",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "init": false,
  "startup": "application",
  "boot": "auto",
  "map": [
    "share:rw"
  ],
  "services": [
    "mqtt:need"
  ],
  "ports": {
    "8099/tcp": 8099
  },
  "ports_description": {
    "8099/tcp": "Web interface"
  },
  "hassio_api": true,
  "hassio_role": "default",
  "webui": "http://[HOST]:[PORT:8099]",
  "codenotary": "bramboe@gmail.com",
  "options": {
    "mqtt_host": null,
    "mqtt_port": null,
    "mqtt_user": null,
    "mqtt_password": null,
    "bleProxies": [],
    "octoDevices": [],
    "target_mac": null,
    "target_pin": null,
    "enable_mac_filtering": true,
    "enable_pin_filtering": true,
    "scan_duration": 30000,
    "scan_interval": 5000,
    "target_device_name": null,
    "ble_scan_parameters": {
      "duration": 30000,
      "interval": 100,
      "window": 50,
      "active": true
    }
  },
  "schema": {
    "mqtt_host": "str?",
    "mqtt_port": "port?",
    "mqtt_user": "str?",
    "mqtt_password": "str?",
    "bleProxies": "list?",
    "octoDevices": "list?",
    "target_mac": "str?",
    "target_pin": "str?",
    "enable_mac_filtering": "bool?",
    "enable_pin_filtering": "bool?",
    "scan_duration": "int?",
    "scan_interval": "int?",
    "target_device_name": "str?",
    "ble_scan_parameters": {
      "duration": "int?",
      "interval": "int?",
      "window": "int?",
      "active": "bool?"
    }
  }
}