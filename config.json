{
  "name": "Octo MQTT",
  "version": "1.2.2",
  "slug": "octo-mqtt",
  "description": "A Home Assistant add-on to enable controlling Octo actuators star version 2.",
  "url": "https://github.com/bramboe/octo-mqtt",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "startup": "application",
  "boot": "auto",
  "host_network": false,
  "init": false,
  "options": {
    "mqtt_host": "localhost",
    "mqtt_port": "1883",
    "mqtt_user": "",
    "mqtt_password": "",
    "bleProxies": [
      {
        "host": "localhost",
        "port": 6053
      }
    ],
    "octoDevices": []
  },
  "schema": {
    "mqtt_host": "str",
    "mqtt_port": "str",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "bleProxies": [
      {
        "host": "str",
        "port": "int"
      }
    ],
    "octoDevices": [
      {
        "name": "str",
        "pin": "str?",
        "friendlyName": "str"
      }
    ]
  }
}