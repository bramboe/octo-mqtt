{
  "name": "Octo MQTT",
  "version": "1.2.4",
  "slug": "octo-mqtt",
  "description": "A Home Assistant add-on to enable controlling Octo actuators star version 2.",
  "url": "https://github.com/bramboe/octo-mqtt/releases/tag/v1.2.4",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "startup": "application",
  "boot": "auto",
  "host_network": false,
  "init": true,
  "ingress": true,
  "ingress_port": 8099,
  "panel_icon": "mdi:bed",
  "panel_title": "Octo Bed Control",
  "options": {
    "mqtt_host": "<auto_detect>",
    "mqtt_port": "<auto_detect>",
    "mqtt_user": "<auto_detect>",
    "mqtt_password": "<auto_detect>",
    "bleProxies": [
      {
        "host": "192.168.2.102",
        "port": 6053
      }
    ],
    "octoDevices": [
      {
        "name": "f6:21:dd:dd:6f:19",
        "friendlyName": "RC2 Bed (DD6F19)",
        "pin": "1234"
      }
    ]
  },
  "schema": {
    "mqtt_host": "str",
    "mqtt_port": "str",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "bleProxies": [
      {
        "host": "str",
        "port": "int?"
      }
    ],
    "octoDevices": [
      {
        "name": "str",
        "pin": "str?",
        "friendlyName": "str"
      }
    ]
  }
}