{
  "name": "Octo MQTT",
  "version": "1.1.5",
  "slug": "octo-mqtt",
  "description": "Octo MQTT integration for Octo actuators star version 2",
  "url": "https://github.com/bramboe/octo-mqtt",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "services": ["mqtt:need"],
  "host_network": true,
  "ports": {
    "1883/tcp": null
  },
  "ports_description": {
    "1883/tcp": "MQTT broker port (only needed if not using Home Assistant's MQTT broker)"
  },
  "map": ["config:ro"],
  "init": false,
  "protection": true,
  "privileged": [],
  "apparmor": true,
  "auth_api": true,
  "ingress": true,
  "ingress_port": 8099,
  "panel_icon": "mdi:bed",
  "panel_title": "Octo MQTT",
  "options": {
    "mqtt": {
      "broker": "mqtt://localhost:1883",
      "username": "",
      "password": ""
    },
    "devices": []
  },
  "schema": {
    "mqtt": {
      "broker": "str",
      "username": "str",
      "password": "str"
    },
    "devices": [
      {
        "name": "str",
        "address": "str"
      }
    ]
  },
  "hassio_api": true,
  "hassio_role": "default"
}