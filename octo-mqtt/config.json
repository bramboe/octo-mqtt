{
  "name": "Octo MQTT",
  "version": "2.7.1",
  "slug": "octo-mqtt",
  "description": "Control Octo smart beds via MQTT and BLE proxy",
  "url": "https://github.com/bramboe/octo-mqtt",
  "startup": "application",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "boot": "auto",
  "services": ["mqtt:need"],
  "host_network": false,
  "map": ["config:ro"],
  "init": false,
  "options": {
    "mqtt_host": "<auto_detect>",
    "mqtt_port": "<auto_detect>",
    "mqtt_user": "<auto_detect>",
    "mqtt_password": "<auto_detect>",
    "bleProxies": [
      {
        "host": "192.168.1.100",
        "port": 6053
      }
    ],
    "octoDevices": []
  },
  "schema": {
    "mqtt_host": "str?",
    "mqtt_port": "str?",
    "mqtt_user": "str?",
    "mqtt_password": "str?",
    "bleProxies": [
      {
        "host": "str",
        "port": "int?"
      }
    ],
    "octoDevices": [
      {
        "name": "str",
        "address": "str",
        "pin": "str?"
      }
    ]
  },
  "ingress": true,
  "ingress_port": 8099,
  "panel_icon": "mdi:bed",
  "panel_title": "Octo MQTT",
  "watchdog": "http://[HOST]:[PORT:8099]/health",
  "timeout": 60,
  "hassio_api": true,
  "hassio_role": "default"
}