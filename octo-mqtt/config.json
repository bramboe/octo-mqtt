{
  "name": "Octo MQTT",
  "version": "1.1.8",
  "slug": "octo_mqtt",
  "description": "Octo MQTT integration for Octo actuators star version 2",
  "url": "https://github.com/bramboe/octo-mqtt",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "host_network": true,
  "ingress": true,
  "panel_icon": "mdi:bed",
  "options": {
    "devices": [],
    "mqtt": {
      "broker": "core-mosquitto",
      "port": 1883,
      "username": "",
      "password": ""
    },
    "esphome": {
      "host": "",
      "port": 6053,
      "password": ""
    }
  },
  "schema": {
    "devices": [
      {
        "name": "str",
        "mac": "str?",
        "pin": "str?"
      }
    ],
    "mqtt": {
      "broker": "str",
      "port": "int",
      "username": "str",
      "password": "str"
    },
    "esphome": {
      "host": "str",
      "port": "int",
      "password": "str"
    }
  }
}