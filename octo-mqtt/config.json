{
  "name": "Octo MQTT",
  "version": "2.0.4",
  "slug": "octo-mqtt",
  "description": "A Home Assistant add-on to enable controlling Octo actuators star version 2. (TypeScript version with BLE scanning)",
  "url": "https://github.com/bramboe/octo-mqtt",
  "startup": "initialize",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "boot": "auto",
  "services": ["mqtt:need"],
  "host_network": false,
  "ports": {
    "8099/tcp": 8099
  },
  "ports_description": {
    "8099/tcp": "Web interface (direct access)"
  },
  "map": ["config:ro"],
  "init": false,
  "protection": true,
  "privileged": [],
  "apparmor": true,
  "auth_api": true,
  "ingress": true,
  "ingress_port": 8099,
  "panel_icon": "mdi:bed",
  "panel_title": "Octo MQTT",
  "watchdog": "http://[HOST]:[PORT:8099]/health",
  "timeout": 60,
  "options": {
    "mqtt_host": "core-mosquitto",
    "mqtt_port": 1883,
    "mqtt_username": "",
    "mqtt_password": "",
    "esphome_proxies": [],
    "octoDevices": [],
    "log_level": "info"
  },
  "schema": {
    "mqtt_host": "str",
    "mqtt_port": "int(1,65536)",
    "mqtt_username": "str?",
    "mqtt_password": "password?",
    "esphome_proxies": [
      {
        "host": "str",
        "port": "int(1,65536)?",
        "password": "password?",
        "encryptionKey": "match(^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)$)?",
        "expectedServerName": "str?"
      }
    ],
    "octoDevices": [
      {
        "name": "str",
        "friendlyName": "str?",
        "pin": "str?"
      }
    ],
    "log_level": "list(debug|info|warn|error)?"
  },
  "hassio_api": true,
  "hassio_role": "default"
}