{
  "name": "octo-mqtt",
  "version": "1.1.5",
  "description": "Octo MQTT integration for Octo actuators star version 2",
  "author": "Bram Boersma",
  "repository": "https://github.com/bramboe/octo-mqtt.git",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=18.19.0"
  },
  "devDependencies": {
    "@types/bonjour": "^3.5.9",
    "@types/express": "^4.17.22",
    "@types/jest": "^26.0.23",
    "@types/mqtt": "^2.5.0",
    "@types/node": "^16.18.126",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "husky": "^6.0.0",
    "jest": "29",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "prebuild": "npm install",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build:ci": "npm run clean && npm run build",
    "clean": "rm -rf dist",
    "lint": "eslint src/ --max-warnings=0",
    "prepare": "husky install",
    "prettier": "prettier --write \"src/**/**.{ts,js,json}\"",
    "test": "jest --watch",
    "test:ci": "jest",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "docker:dev": "docker build -t smqtt . && docker run -v $(pwd)/../data:/data smqtt",
    "cachebust": "echo Build-$(date +%s)"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*"
  ],
  "lint-staged": {
    "src/*.ts": "eslint --max-warnings=0",
    "srs/*.{ts,js,json}": "prettier --write"
  },
  "dependencies": {
    "@2colors/esphome-native-api": "^1.3.2",
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.7.4",
    "express": "^4.21.2",
    "mqtt": "^4.2.8",
    "node-fetch": "^2.6.7",
    "ws": "^8.18.2"
  }
}
